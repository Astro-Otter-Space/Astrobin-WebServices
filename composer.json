{
  "name" : "hamhamfonfon/astrobin-ws",
  "license": "MIT",
  "version": "2.4.5",
  "description" : "Astrobin WebServices for PHP projects",
  "type" : "library",
  "authors" : [{
    "name" : "Stéphane Méaudre",
    "email" : "balistik.fonfon@gmail.com"
  }],
  "keywords" : [
    "Astrobin WebServices", "API", "astronomy", "Deep space objects"
  ],
  "support": {
    "issues": "https://github.com/HamHamFonFon/Astrobin-WebServices/issues"
  },
  "config": {
    "platform": {
      "php": "7.4"
    }
  },
  "require" : {
    "php": ">=7.4.0",
    "guzzlehttp/guzzle": "^7.4",
    "ext-ctype": "*",
    "ext-json": "*"
  },
  "autoload" : {
    "psr-4" : {
      "AstrobinWs\\" : "src/"
    },
    "classmap": [
      "src/"
    ]
  },
  "target-dir" : "",
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.2",
    "phpunit/phpunit": "^9.3",
    "dg/bypass-finals": "^1.3"
  }
}
